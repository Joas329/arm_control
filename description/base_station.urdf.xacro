	<xacro:arg name="namespace" default="robot1"/>

    <!-- ################# Base Link ################# -->
    <link name="$(arg namespace)_base_link">
        <inertial>
            <origin xyz="-0.038799 5.6771E-08 0.039037" rpy="0 0 0"/>
            <mass value="4.037"/>
            <inertia
                ixx="0.0047485"
                ixy="6.9936E-08"
