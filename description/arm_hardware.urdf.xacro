	<!--Defining ROS2 Control Joints-->

	<ros2_control name="yurs_motor" type="actuator">
		<hardware>
			<plugin>yurs_hardware/yurs_motor</plugin>
			<param name="can_interface">can0</param>
		</hardware>

 		<joint name="arm_j1">
			<param name="device_id">6</param>
			<param name="counts_per_rev">1050</param>
		</joint>
		<joint name="arm_j2">
			<param name="device_id">7</param>
			<param name="counts_per_rev">5250</param>
		</joint>
		<joint name="arm_j3">
			<param name="device_id">8</param>
			<param name="counts_per_rev">5250</param>
		</joint>
		<joint name="arm_j4">
			<param name="device_id">9</param>
			<param name="counts_per_rev">1149.1597</param>
		</joint>
		<joint name="arm_j5">
