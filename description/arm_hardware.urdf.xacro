<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robot_arm" >	

	<xacro:arg name="namespace" default="robot1"/>
	<xacro:include filename="macros.xacro"/>

	<material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>

	<!--Dummy Link-->
	<link name="$(arg namespace)_base_link"/>
    <joint name="$(arg namespace)_dummy_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="$(arg namespace)_base_link"/>
        <child link="$(arg namespace)_arm_a1"/>
    </joint>

	<!--Arm Links-->

	<!--Arm 1-->

	<xacro:arm_component 
		namespace="$(arg namespace)"
		component_number ="1"
		type = "revolute"
		parent_link="$(arg namespace)_arm_a1"
		child_link="$(arg namespace)_arm_a2"
		mass = "3.8185"
		axis = "0 0 1"
		Lxx="100.85864"
		Lxy="2.33423"
