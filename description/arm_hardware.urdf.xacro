	<!--Arm 7-->	

	<link name="$(arg namespace)_arm_a7">
		<inertial>
		<mass value="0.454"/>
		<origin rpy="0 0 0" xyz="0.0 0.0 0.0"/>
		<inertia ixx="7.0307" ixy="0.0" ixz="-0.0" iyy="7.0307" iyz="-0.0" izz="7.0307"/>
