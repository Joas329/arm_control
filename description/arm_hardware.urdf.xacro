		inertiaX = "-3.34872"
		inertiaY = "-7.71683"
		inertiaZ = "0.00031"
		effort_limit = "400000.0"
		lower_limit = "${radians(-180)}"
		upper_limit = "${radians(180)}"
		velocity_limit = "300000.0"
		joint_X = "-3.50000000"
		joint_Y = "0.51712500"
		joint_Z = "0.0"
	/>


