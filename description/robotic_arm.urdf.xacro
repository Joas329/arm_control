        axis="0 -1 0"
        Lxx="0.0010278"
        Lxy="-3.2684E-07"
        Lxz="-5.2705E-07"
        Lyy="0.001187"
        Lyz="2.9768E-06"
        Lzz="0.00070562"
        file="Link1"
        inertiaX="0.0010278"
        inertiaY="-3.2684E-07"
        inertiaZ="-5.2705E-07"
        effort_limit="10"
        lower_limit="${radians(-3.14)}"
        upper_limit="${radians(3.14)}"
