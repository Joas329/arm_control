<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="robotic_arm" >	

	<xacro:arg name="namespace" default="robot1"/>
	<xacro:include filename="macros.xacro"/>

	<material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="orange">
        <color rgba="1 0.3 0.1 1"/>
    </material>

    <material name="blue">
        <color rgba="0.2 0.2 1 1"/>
    </material>

    <material name="black">
        <color rgba="0 0 0 1"/>
    </material>


    	<!--Arm Links-->

    <!--Dummy Link-->
	<link name="$(arg namespace)_pre_base_link"/>
    <joint name="$(arg namespace)_dummy_joint" type="fixed">
        <origin xyz="0 0 0" rpy="0 0 0"/>
        <parent link="$(arg namespace)_pre_base_link"/>
        <child link="$(arg namespace)_base_link"/>
    </joint>

	<!--Link 1-->
    <xacro:arm_component 
        namespace="$(arg namespace)"
        component_number="1"
        type="revolute"
        parent_link="$(arg namespace)_base_link"
        child_link="$(arg namespace)_Link1"
